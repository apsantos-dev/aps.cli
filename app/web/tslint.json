{
  "extends": "tslint:recommended",
  "rulesDirectory": ["codelyzer"],
  "rules": {
    "max-classes-per-file": false,
    "max-line-length": [true, 120],
    "no-console": [
      true,
      "clear",
      "log",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "quotemark": [true, "single"],
    "removeComments": true
  }
}
